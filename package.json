{
  "name": "greatgamegal.com-html",
  "version": "1.0.0",
  "author": "Serenity Braesch",
  "main": "index.js",
  "devDependencies": {
    "@types/web": "^0.0.137",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@typescript-eslint/typescript-estree": "^6.20.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2"
  },
  "description": "Frontend for greatgamegal.com.",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lintAndFix": "prettier --write --log-level warn . && eslint --fix .",
    "build": "tsc && bun run build.ts",
    "dev": "nodemon --exec 'bun run lintAndFix && bun run build'"
  },
  "nodemonConfig": {
    "ignore": [
      "build"
    ],
    "delay": 1000
  },
  "dependencies": {
    "@types/bun": "^1.0.4"
  }
}
