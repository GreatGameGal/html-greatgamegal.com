{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "rootDir": "./src/",
    "moduleResolution": "classic",
    "outDir": "./build/",
    "newLine": "lf",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  }
}
